Laguerre3 {
init:
  z = pixel
  i = sqrt(-1)
loop:
  deg = 3
  f = z^3 - 1
  f1 = 3*z^2
  f2 = 6*z
  zold = z
  G = f1/f
  H = G^2 - (f2/f)
  x = G + sqrt((deg-1)*(deg*H - G^2))
  y = G - sqrt((deg-1)*(deg*H - G^2))
  if (|x| > |y|)
     z = z - deg/x
